PREFIX : <urn:example:/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

# This is the way LDO describes the shape of data using the following syntax: https://w3c.github.io/shacl/shacl-compact-syntax/
shape :List {
    :item IRI @:Item .
}

shape :Item {
    :website IRI [0..1] .
    :name xsd:string [0..1] .
    :description xsd:string [0..1] .
    :featured xsd:boolean [0..1] .
    :thumbnail IRI [0..1] .
}
